{
    "name": "studip/core",
    "description": "Stud.IP ist ein Campus- und Lernmanagementsystem für Hochschulen, Schulen, Unternehmen, Verbände und Behörden.",
    "config": {
        "platform": {
            "php": "7.0.33"
        },
        "vendor-dir": "composer"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/locales",
                "version":"1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/locales.git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "citation-style-language/styles-distribution",
                "version":"1.0.0",
                "source": {
                    "type": "git",
                    "url": "https://github.com/citation-style-language/styles-distribution.git",
                    "reference": "master"
                }
            }
        }
    ],
    "require-dev": {
        "adlawson/vfs": "~0.12.1",
        "camspiers/json-pretty": "~1.0.2",
        "codeception/codeception": "4.0.2",
        "monolog/monolog": "~1.21.0",
        "php-http/curl-client": "~1.7.0",
        "phpunit/phpunit": "6.5.14",
        "woohoolabs/yang": "~0.9.0",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-asserts": "^1.1"
    },
    "require": {
        "php": "^7.0",
        "guzzlehttp/psr7": "~1.4.2",
        "neomerx/json-api": "~1.0.9",
        "slim/slim": "~3.12.3",
        "spomky-labs/otphp": "^8.3.3",
        "tuupola/cors-middleware": "~0.5.2",
        "citation-style-language/locales":"@dev",
        "citation-style-language/styles-distribution":"@dev",
        "seboettg/citeproc-php": "^2",
        "tecnickcom/tcpdf": "^6.3",
        "scssphp/scssphp": "^1.4",
        "symfony/yaml": "^3.4",
        "phpwhois/idna-convert": "^0.9.0",
        "mishal/iless": "^2.2",
        "rashid2538/php-htmldiff": "dev-master",
        "ezyang/htmlpurifier": "^4.13",
        "davefx/phplot": "^6.2",
        "jasig/phpcas": "^1.3",
        "phpxmlrpc/phpxmlrpc": "^4.4",
        "gossi/docblock": "^1.6"
    }
